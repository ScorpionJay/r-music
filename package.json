  "main": "index.js",
  "scripts": {
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack",
    "build:prod":"rimraf dist & webpack --config config/webpack.prod.js",
    "build:dev":"rimraf dev & webpack --config config/webpack.dev.js",
    "build:hash":"rimraf dev & webpack --config config/webpack.hash.js",
    "build": "npm run build:prod",
    "clean:dist": "rimraf dist",
    "clean:dev": "rimraf dev",
    "test": "jest",
    "start": "webpack-dev-server --config webpack.config.js --hot",
    "serve:prod": "webpack-dev-server --config config/webpack.prod.js --hot"
